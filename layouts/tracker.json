{
    "tracker_default": {
        "type": "container",
        "background": "#00000000",
        "content": {
            "type": "dock",
            "dropshadow": true,
            "content": [
                {
                    "type": "dock",
                    "dock": "left",
                    "content": [
                        {
                            "type": "group",
                            "header": "Items",
                            "dock": "top",
                            "margin": "0,0,3,0",
                            "content": {
                                "type": "layout",
                                "h_alignment": "center",
                                "v_alignment": "center",
                                "key": "shared_item_grid_horizontal"
                            }
                        },
                        {
                            "type": "group",
                            "header": ":)",
                            "content": {
                                "style": "wrap",
                                "type": "recentpins",
                                "orientation": "vertical",
                                "compact": true
                            }
                        }
                    ]
                },
                {
                    "type": "dock",
                    "content": {
                        "type": "layout",
                        "key": "tabbed_maps_vertical"
                    }
                }
            ]
        }
    },
    "tracker_horizontal": {
        "type": "container",
        "background": "#00000000",
        "content": {
            "type": "dock",
            "dropshadow": true,
            "content": [
                {
                    "type": "dock",
                    "dock": "bottom",
                    "content": [
                        {
                            "type": "group",
                            "header": "Items",
                            "dock": "left",
                            "margin": "0,0,3,0",
                            "content": {
                                "type": "layout",
                                "h_alignment": "center",
                                "v_alignment": "center",
                                "key": "shared_item_grid_horizontal"
                            }
                        },
                        {
                            "type": "group",
                            "header": "Tracker by ZobeePlays",
                            "content": {
                                "style": "wrap",
                                "type": "recentpins",
                                "orientation": "horizontal",
                                "compact": true
                            }
                        }
                    ]
                },
                {
                    "type": "dock",
                    "content": {
                        "type": "layout",
                        "key": "tabbed_maps_horizontal"
                    }
                }
            ]
        }
    }
}